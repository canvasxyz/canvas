{
  "private": true,
  "name": "@canvas-js/test-network",
  "version": "0.9.0",
  "type": "module",
  "author": "Canvas Technologies, Inc. (https://canvas.xyz)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "build": "concurrently \"npm:*:build\"",
    "dev": "concurrently \"npm:*:dev\"",
    "dashboard:client:build": "vite build --outDir dist/dashboard/client",
    "dashboard:client:dev": "vite build --outDir dist/dashboard/client --watch",
    "dashboard:server:build": "esbuild lib/dashboard/server/index.js --bundle --outdir=dist/dashboard/server --platform=node --target=node20 --format=esm",
    "dashboard:server:dev": "esbuild lib/dashboard/server/index.js --bundle --outdir=dist/dashboard/server --platform=node --target=node20 --format=esm --watch",
    "peer:build": "esbuild lib/peer/index.js --bundle --outdir=dist/peer --platform=node --target=node20 --format=esm --banner:js=\"import {createRequire as yix6bKft} from 'module';const require=yix6bKft(import.meta.url);\"",
    "peer:dev": "esbuild lib/peer/index.js --bundle --outdir=dist/peer --platform=node --target=node20 --format=esm --watch"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.2",
    "vite": "^5.2.10"
  },
  "dependencies": {
    "@canvas-js/gossiplog": "0.9.0",
    "@canvas-js/interfaces": "0.9.0",
    "@chainsafe/libp2p-gossipsub": "^11.0.1",
    "@chainsafe/libp2p-noise": "^14.1.0",
    "@libp2p/bootstrap": "^10.0.7",
    "@libp2p/fetch": "^1.0.5",
    "@libp2p/identify": "^1.0.6",
    "@libp2p/interface": "^1.0.2",
    "@libp2p/kad-dht": "^12.0.4",
    "@libp2p/logger": "^4.0.2",
    "@libp2p/mplex": "^10.0.7",
    "@libp2p/peer-id": "^4.0.2",
    "@libp2p/peer-id-factory": "^4.0.1",
    "@libp2p/ping": "^1.0.6",
    "@libp2p/prometheus-metrics": "^3.0.7",
    "@libp2p/websockets": "^8.0.7",
    "@multiformats/multiaddr": "^12.1.11",
    "@multiformats/multiaddr-matcher": "^1.1.0",
    "@noble/hashes": "^1.4.0",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "d3": "^7.9.0",
    "express": "^4.18.2",
    "libp2p": "^1.0.10",
    "multiformats": "^13.1.0",
    "p-queue": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
