{
  "private": true,
  "name": "@canvas-js/test-network",
  "version": "0.10.0-beta.1",
  "type": "module",
  "author": "Canvas Technologies, Inc. (https://canvas.xyz)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "build": "concurrently \"npm:*:build\"",
    "dev": "concurrently \"npm:*:dev\"",
    "dashboard:client:build": "vite build --outDir dist/dashboard/client",
    "dashboard:client:dev": "npm run dashboard:client:build -- --watch",
    "dashboard:server:build": "esbuild lib/dashboard/server/index.js --bundle --outdir=dist/dashboard/server --platform=node --target=node20 --format=esm --banner:js=\"import { createRequire as yix6bKft } from 'module'; const require = yix6bKft(import.meta.url);\"",
    "dashboard:server:dev": "npm run dashboard:server:build -- --watch",
    "peer:build": "esbuild lib/peer/index.js --bundle --outdir=dist/peer --platform=node --target=node20 --format=esm --external:@canvas-js/okra-node --banner:js=\"import { createRequire as yix6bKft } from 'module'; const require = yix6bKft(import.meta.url);\"",
    "peer:dev": "npm run peer:build -- --watch",
    "peer-browser:build": "esbuild lib/peer-browser/client/index.js --bundle --outdir=dist/peer-browser/client --platform=browser --target=esnext --format=esm",
    "peer-browser:dev": "npm run peer-browser:build -- --watch",
    "relay:build": "esbuild lib/relay/index.js --bundle --outdir=dist/relay --platform=node --target=node20 --format=esm --banner:js=\"import { createRequire as yix6bKft } from 'module'; const require = yix6bKft(import.meta.url);\"",
    "relay:dev": "npm run relay:build -- --watch"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.2",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "@canvas-js/gossiplog": "0.10.0-beta.1",
    "@canvas-js/interfaces": "0.10.0-beta.1",
    "@chainsafe/libp2p-gossipsub": "^13.0.0",
    "@chainsafe/libp2p-noise": "^15.0.0",
    "@chainsafe/libp2p-yamux": "^6.0.2",
    "@ipld/dag-cbor": "^9.2.0",
    "@libp2p/bootstrap": "^10.0.24",
    "@libp2p/circuit-relay-v2": "^1.0.24",
    "@libp2p/fetch": "^1.0.18",
    "@libp2p/identify": "^2.0.2",
    "@libp2p/interface": "^1.3.1",
    "@libp2p/kad-dht": "^12.0.17",
    "@libp2p/logger": "^4.0.13",
    "@libp2p/peer-id": "^4.1.2",
    "@libp2p/peer-id-factory": "^4.1.1",
    "@libp2p/ping": "^1.0.19",
    "@libp2p/plaintext": "^1.0.24",
    "@libp2p/webrtc": "^4.0.33",
    "@libp2p/websockets": "^8.0.24",
    "@multiformats/multiaddr": "^12.1.11",
    "@multiformats/multiaddr-matcher": "^1.2.1",
    "@noble/hashes": "^1.4.0",
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "isomorphic-ws": "^5.0.0",
    "libp2p": "^1.6.0",
    "multiformats": "^13.1.0",
    "p-queue": "^8.0.0",
    "puppeteer": "^22.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "ws": "^8.17.0"
  }
}
