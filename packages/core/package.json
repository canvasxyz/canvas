{
  "name": "@canvas-js/core",
  "version": "0.2.2",
  "author": "Canvas Technology Corporation (https://canvas.xyz)",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib",
    "rpc",
    "types"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "imports": {
    "#sqlite3": {
      "types": "./sqlite/sqlite3-bundler-friendly.mts",
      "default": "./sqlite/sqlite3-bundler-friendly.mjs"
    }
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./rpc/sync": {
      "default": "./rpc/sync/index.cjs"
    },
    "./sync": {
      "import": "./lib/sync/index.js"
    },
    "./utils": {
      "import": "./lib/utils.js"
    },
    "./constants": {
      "import": "./lib/constants.js"
    },
    "./codecs": {
      "import": "./lib/codecs.js"
    },
    "./errors": {
      "import": "./lib/errors.js"
    },
    "./api": {
      "import": "./lib/api.js"
    },
    "./components/messageStore": {
      "import": {
        "browser": "./lib/components/messageStore/browser/index.js",
        "node": "./lib/components/messageStore/node/index.js"
      }
    },
    "./components/modelStore": {
      "import": {
        "browser": "./lib/components/modelStore/browser/index.js",
        "node": "./lib/components/modelStore/node/index.js"
      }
    },
    "./components/libp2p": {
      "import": {
        "browser": "./lib/components/libp2p/browser/index.js",
        "node": "./lib/components/libp2p/node/index.js"
      }
    },
    "./components/vm": {
      "import": "./lib/components/vm/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "generate-rpc": "npm run generate-rpc-js && npm run generate-rpc-ts",
    "generate-rpc-js": "pbjs ./rpc/sync/index.proto -t static-module -w commonjs -o ./rpc/sync/index.cjs",
    "generate-rpc-ts": "pbjs -t static-module ./rpc/sync/index.proto | pbts -o ./rpc/sync/index.d.cts -",
    "test": "ava"
  },
  "dependencies": {
    "@canvas-js/chain-ethereum": "0.2.2",
    "@canvas-js/interfaces": "0.2.2",
    "@canvas-js/okra-browser": "^0.1.3",
    "@canvas-js/okra-node": "^0.2.1",
    "@chainsafe/libp2p-gossipsub": "^6.2.0",
    "@chainsafe/libp2p-noise": "^11.0.1",
    "@hyperjump/json-schema": "^1.2.4",
    "@libp2p/bootstrap": "^6.0.0",
    "@libp2p/interface-connection": "^3.0.8",
    "@libp2p/interface-peer-id": "^2.0.1",
    "@libp2p/interface-registrar": "^2.0.7",
    "@libp2p/interfaces": "^3.2.0",
    "@libp2p/kad-dht": "^7.0.0",
    "@libp2p/logger": "^2.0.5",
    "@libp2p/mplex": "^7.1.1",
    "@libp2p/peer-id-factory": "^2.0.0",
    "@libp2p/prometheus-metrics": "^1.1.3",
    "@libp2p/websockets": "^5.0.4",
    "@multiformats/multiaddr": "^11.5.0",
    "@noble/hashes": "^1.2.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/k-bucket": "^5.0.1",
    "better-sqlite3": "^8.0.1",
    "chalk": "^5.2.0",
    "ethers": "^5.7.1 <6",
    "express": "^4.18.2",
    "fp-ts": "^2.13.1",
    "http-status-codes": "^2.2.0",
    "io-ts": "^2.2.20",
    "ipfs-only-hash": "^4.0.0",
    "it-pushable": "^3.1.2",
    "it-stream-types": "^1.0.5",
    "k-bucket": "^5.1.0",
    "libp2p": "^0.42.0",
    "multiformats": "^11.0.0",
    "p-queue": "^7.3.0",
    "prom-client": "^14.2.0",
    "protobufjs": "~7.2.2",
    "quickjs-emscripten": "^0.22.0",
    "safe-stable-stringify": "^2.4.2",
    "timeout-abort-controller": "^3.0.0",
    "uint8arraylist": "^2.4.3",
    "uuid": "^9.0.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.4",
    "ava": "^5.1.0",
    "dotenv": "^16.0.3",
    "nanoid": "^4.0.0",
    "protobufjs-cli": "~1.1.1",
    "stream-to-it": "^0.2.4",
    "typescript": "^4.9.4"
  }
}
