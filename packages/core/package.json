{
  "name": "@canvas-js/core",
  "version": "0.4.7",
  "author": "Canvas Technologies, Inc. (https://canvas.xyz)",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib",
    "rpc",
    "sqlite",
    "types"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "imports": {
    "#sqlite3": "./sqlite/sqlite3-bundler-friendly.mjs",
    "#rpc/sync": "./rpc/sync/index.cjs"
  },
  "exports": {
    ".": "./lib/index.js",
    "./sync": "./lib/sync/index.js",
    "./utils": "./lib/utils.js",
    "./constants": "./lib/constants.js",
    "./codecs": "./lib/codecs.js",
    "./errors": "./lib/errors.js",
    "./bootstrap": "./lib/bootstrap.js",
    "./api": "./lib/api.js",
    "./components/vm": "./lib/components/vm/index.js",
    "./components/messageStore": {
      "browser": "./lib/components/messageStore/browser/index.js",
      "default": "./lib/components/messageStore/node/index.js"
    },
    "./components/modelStore": {
      "browser": "./lib/components/modelStore/browser/index.js",
      "default": "./lib/components/modelStore/node/index.js"
    },
    "./components/libp2p": {
      "browser": "./lib/components/libp2p/browser/index.js",
      "default": "./lib/components/libp2p/node/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "generate-rpc": "npm run generate-rpc-js && npm run generate-rpc-ts",
    "generate-rpc-js": "pbjs ./rpc/sync/index.proto -t static-module -w commonjs -o ./rpc/sync/index.cjs",
    "generate-rpc-ts": "pbjs -t static-module ./rpc/sync/index.proto | pbts -o ./rpc/sync/index.d.cts -",
    "test": "ava",
    "benchmark": "RUN_BENCHMARKS=1 ava test/benchmarks.test.ts"
  },
