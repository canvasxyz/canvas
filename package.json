{
  "name": "canvas",
  "private": true,
  "version": "0.0.52",
  "type": "module",
  "scripts": {
    "dev": "tsc --build --watch",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "publish": "npm run build && npm publish --workspace=packages --access public",
    "test": "npm run test --workspace=@canvas-js/core --workspace=@canvas-js/interfaces",
    "chain-test": "npm run test --workspace=@canvas-js/interfaces"
  },
  "workspaces": [
    "packages/interfaces",
    "packages/core",
    "packages/cli",
    "packages/next",
    "packages/hooks",
    "packages/chain-ethereum",
    "packages/chain-cosmos",
    "packages/chain-solana",
    "packages/chain-substrate",
    "examples/app-store",
    "examples/chat-webpack",
    "examples/chat-next"
  ]
}
