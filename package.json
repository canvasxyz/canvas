{
	"name": "canvas",
	"private": true,
	"version": "0.16.0-next.2",
	"type": "module",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev": "tsc --build --watch",
		"build": "tsc --build && npm run build -w packages/network-explorer",
		"clean": "tsc --build --clean",
		"clean-all": "rm -rf contracts/lib/* contracts/test/lib/* packages/*/lib/* packages/*/test/lib/* packages/*/tsconfig.tsbuildinfo packages/network-explorer/dist/* packages/ethereum-contracts/artifacts/* packages/ethereum-contracts/typesigner-types/* examples/*/lib/* examples/*/tsconfig.tsbuildinfo examples/*/tsconfig.node.tsbuildinfo",
		"lint": "eslint .",
		"publish": "npm run clean && npm run build && npm publish -w=packages/ --access public",
		"test": "npm run test -w=packages/interfaces -w=packages/modeldb -w=packages/gossiplog -w=packages/core -w=packages/vm -w packages/signer-atp -w=packages/signer-cosmos -w=packages/signer-ethereum -w=packages/signer-ethereum-viem -w=packages/signer-solana -w=packages/signer-substrate",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"contracts:build": "npm run build -w packages/ethereum-contracts",
		"contracts:clean": "npm run clean -w packages/ethereum-contracts",
		"contracts:test": "npm run test -w packages/ethereum-contracts"
	},
	"workspaces": [
		"packages/*",
		"examples/*"
	],
	"devDependencies": {
		"@ava/typescript": "^5.0.0",
		"@canvas-js/cli": "0.16.0-next.2",
		"@canvas-js/hooks": "0.16.0-next.2",
		"@canvas-js/interfaces": "0.16.0-next.2",
		"@canvas-js/modeldb": "0.16.0-next.2",
		"@canvas-js/modeldb-durable-objects": "0.16.0-next.2",
		"@canvas-js/modeldb-idb": "0.16.0-next.2",
		"@canvas-js/modeldb-pg": "0.16.0-next.2",
		"@canvas-js/modeldb-sqlite": "0.16.0-next.2",
		"@canvas-js/modeldb-sqlite-expo": "0.16.0-next.2",
		"@canvas-js/modeldb-sqlite-wasm": "0.16.0-next.2",
		"@canvas-js/signatures": "0.16.0-next.2",
		"@canvas-js/signer-atp": "0.16.0-next.2",
		"@canvas-js/signer-cosmos": "0.16.0-next.2",
		"@canvas-js/signer-ethereum": "0.16.0-next.2",
		"@canvas-js/signer-ethereum-viem": "0.16.0-next.2",
		"@canvas-js/signer-solana": "0.16.0-next.2",
		"@canvas-js/signer-substrate": "0.16.0-next.2",
		"@eslint/compat": "^1.2.7",
		"@eslint/eslintrc": "^3.3.0",
		"@eslint/js": "^9.21.0",
		"@types/node": "^22.13.1",
		"@typescript-eslint/eslint-plugin": "^8.25.0",
		"@typescript-eslint/parser": "^8.25.0",
		"@vueuse/motion": "^2.2.3",
		"ava": "^6.2.0",
		"eslint": "^9.21.0",
		"eslint-plugin-import": "^2.31.0",
		"ethers": "^6.13.2",
		"markdown-it-checkbox": "^1.1.0",
		"markdown-it-footnote": "^3.0.3",
		"markdown-it-named-code-blocks": "^0.2.0",
		"pg": "^8.12.0",
		"prettier": "^3.5.3",
		"protons": "^7.6.0",
		"protons-runtime": "^5.5.0",
		"react": "^18.3.1",
		"typescript": "~5.6.0",
		"veaury": "^2.4.0",
		"vite": "^6.3.2",
		"vitepress": "^2.0.0-alpha.4",
		"vue": "^3.3.7",
		"vue-markdown-render": "^2.2.1"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "4.13.0"
	},
	"dependencies": {
		"floating-vue": "^5.2.2",
		"react-icons": "^5.5.0",
		"vue3-icons": "^0.1.13"
	}
}
