{
  "name": "canvas",
  "private": true,
  "version": "0.5.1",
  "type": "module",
  "scripts": {
    "dev": "tsc --build --watch",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "lint": "eslint .",
    "publish": "npm run clean && npm run build && npm publish -w=packages/ --access public",
    "test": "npm run test -w=packages/interfaces -w=packages/modeldb -w=packages/gossiplog -w=packages/core -w=packages/vm",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@canvas-js/chain-ethereum": "^0.5.1",
    "@canvas-js/hooks": "^0.5.1",
    "@canvas-js/templates": "^0.5.1",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vueuse/motion": "^2.0.0",
    "ava": "^5.3.1",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.29.0",
    "ethers": "^6.8.1",
    "markdown-it-named-code-blocks": "^0.2.0",
    "react": "^18.2.0",
    "react-chessboard": "^4.3.2",
    "typescript": "^5.2.2",
    "veaury": "^2.3.12",
    "vitepress": "^1.0.0-rc.24",
    "vue": "^3.3.7",
    "vue-markdown-render": "^2.1.1"
  },
  "dependencies": {
    "@chrisoakman/chessboardjs": "^1.0.0",
    "chess.js": "^0.12.1",
    "chessboardjsx": "^2.4.7"
  }
}
