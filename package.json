{
  "name": "canvas",
  "private": true,
  "version": "0.0.33",
  "type": "module",
  "scripts": {
    "dev": "tsc --build --watch",
    "build": "tsc --build && npm run build --workspace=@canvas-js/example-chat-next --workspace=@canvas-js/example-chat-webpack --workspace=@canvas-js/hub",
    "clean": "tsc --build --clean",
    "version": "npm version --git-tag-version=false --workspaces=true --include-workspace-root=true --workspaces-update=true --save --save-exact",
    "publish": "npm publish --workspace=packages --access public",
    "test": "npm run test --workspace=@canvas-js/core"
  },
  "workspaces": [
    "packages/interfaces",
    "packages/core",
    "packages/cli",
    "packages/next",
    "packages/hooks",
    "packages/hub",
    "packages/bootstrap-peer",
    "examples/app-store",
    "examples/chat-webpack",
    "examples/chat-next"
  ]
}
