{
	"private": true,
	"name": "@canvas-js/chat-example",
	"version": "0.15.1",
	"type": "module",
	"scripts": {
		"build": "npx vite build",
		"build:watch": "npx vite build --watch",
		"dev": "npx vite",
		"dev:server": "source .env && canvas run --port 8080 --static dist --network-explorer --admin $ADMIN_ADDRESS --topic $TOPIC src/contract.ts",
		"dev:server:persistent": "source .env && canvas run --port 8080 --static dist --network-explorer --admin $ADMIN_ADDRESS --topic $TOPIC --init src/contract.ts /tmp/chat-example",
		"dev:server:reset": "rm -rf /tmp/chat-example"
	},
	"dependencies": {
		"@canvas-js/cli": "0.15.1",
		"@canvas-js/core": "0.15.1",
		"@canvas-js/gossiplog": "0.15.1",
		"@canvas-js/hooks": "0.15.1",
		"@canvas-js/interfaces": "0.15.1",
		"@canvas-js/modeldb-sqlite-wasm": "0.15.1",
		"@canvas-js/signer-atp": "0.15.1",
		"@canvas-js/signer-cosmos": "0.15.1",
		"@canvas-js/signer-ethereum": "0.15.1",
		"@canvas-js/signer-ethereum-viem": "0.15.1",
		"@canvas-js/signer-solana": "0.15.1",
		"@canvas-js/signer-substrate": "0.15.1",
		"@cosmjs/encoding": "^0.32.3",
		"@farcaster/auth-kit": "^0.6.0",
		"@farcaster/frame-sdk": "^0.0.34",
		"@keplr-wallet/types": "^0.11.64",
		"@libp2p/interface": "^2.10.3",
		"@magic-ext/auth": "^4.3.2",
		"@multiformats/multiaddr": "^12.4.1",
		"@noble/hashes": "^1.8.0",
		"@polkadot/extension-dapp": "^0.46.9",
		"@terra-money/feather.js": "^2.0.4",
		"@types/react": "^18.3.9",
		"@types/react-dom": "^18.3.0",
		"buffer": "^6.0.3",
		"comlink": "^4.4.1",
		"ethers": "^6.13.5",
		"idb": "^8.0.3",
		"magic-sdk": "^21.5.0",
		"multiformats": "^13.3.7",
		"process": "^0.11.10",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-virtuoso": "^4.12.7",
		"viem": "^2.22.21",
		"vite-plugin-wasm": "^3.3.0",
		"web3": "^4.10.0"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.38",
		"rollup-plugin-visualizer": "^5.14.0",
		"tailwindcss": "^3.4.4",
		"typescript": "~5.6.0",
		"vite": "^6.3.2",
		"vite-plugin-node-polyfills": "^0.23.0"
	}
}
