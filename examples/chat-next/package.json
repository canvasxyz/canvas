    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.1",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^4.1.0",
