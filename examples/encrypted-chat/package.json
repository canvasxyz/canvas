{
	"private": true,
	"name": "@canvas-js/encrypted-chat-example",
	"version": "0.14.0-next.2",
	"type": "module",
	"scripts": {
		"build": "npx vite build",
		"build:watch": "npx vite build --watch",
		"dev": "npx vite",
		"dev:server": "canvas run --port 8080 --static dist --network-explorer --admin 0x34C3A5ea06a3A67229fb21a7043243B0eB3e853f --topic encrypted-chat-example.canvas.xyz src/contract.ts",
		"dev:server:persistent": "canvas run --port 8080 --static dist --network-explorer --admin 0x34C3A5ea06a3A67229fb21a7043243B0eB3e853f --topic encrypted-chat-example.canvas.xyz --init src/contract.ts /tmp/encrypted-chat-example",
    "dev:server:reset": "rm -rf /tmp/encrypted-chat-example"
	},
	"dependencies": {
		"@canvas-js/chain-ethereum": "0.14.0-next.2",
		"@canvas-js/cli": "0.14.0-next.2",
		"@canvas-js/core": "0.14.0-next.2",
		"@canvas-js/hooks": "0.14.0-next.2",
		"@metamask/eth-sig-util": "^7.0.1",
		"ethers": "^6.13.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@types/react": "^18.3.9",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^8.25.0",
		"@typescript-eslint/parser": "^8.25.0",
		"@vitejs/plugin-react": "^4.3.1",
		"eslint": "^9.21.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"typescript": "~5.6.0",
		"vite": "^5.4.8",
		"vite-plugin-node-polyfills": "^0.22.0"
	}
}
